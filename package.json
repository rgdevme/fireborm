    "type": "git",
    "url": "git+https://github.com/rgdevme/fireborm.git"
  },
  "keywords": [
    "firebase",
    "admin",
    "functions",
    "orm"
  ],
  "author": "Rafael Gonz√°lez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rgdevme/fireborm/issues"
  },
  "homepage": "https://github.com/rgdevme/fireborm#readme",
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0"
  },
  "peerDependencies": {
    "firebase": "^10.5.0"
  }
