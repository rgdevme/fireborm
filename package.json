{
  "name": "fireborm",
  "version": "0.0.1",
  "description": "A Firebase ORM-like wrapper, inspired by packages like sequelize and mongoose",
  "type": "module",
  "source": "src/index.ts",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
