{
  "name": "fireborm",
  "version": "0.3.2",
  "description": "A Firebase ORM-like wrapper, inspired by packages like sequelize and mongoose",
  "type": "module",
  "source": "src/index.ts",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "watch": "vite build -w",
    "bump": "npx bumpp",
    "doc": "npx typedoc"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "^npm@10.8.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgdevme/fireborm.git"
  },
  "keywords": [
    "firebase",
    "admin",
    "functions",
    "orm"
  ],
  "author": "Rafael Gonz√°lez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rgdevme/fireborm/issues"
  },
  "homepage": "https://github.com/rgdevme/fireborm#readme",
  "devDependencies": {
    "bumpp": "^10.2.3",
    "typedoc": "^0.28.12",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "5.9.2",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "firebase": "^12.2.1"
  }
}
